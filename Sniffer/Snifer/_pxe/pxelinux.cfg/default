DEFAULT menu.c32
PROMPT 0
ALLOWOPTIONS 0
TIMEOUT 100
#NOESCAPE 1

#MENU SHIFTKEY 1
MENU TITLE PXE Boot Menu

LABEL local
MENU LABEL Local Boot
MENU DEFAULT
localboot 0


LABEL floppies
	MENU LABEL > Floppies
	KERNEL menu.c32
	APPEND menuconf/floppies.conf

LABEL bootutils
	MENU LABEL > Utilitaires de ^boot
	KERNEL menu.c32
	APPEND menuconf/bootutil.conf

LABEL Install
	MENU LABEL > Install CD's
	KERNEL menu.c32
	APPEND menuconf/install.conf
	
LABEL hddUtils
	MENU LABEL > Utilitaires de ^disques dur
	KERNEL menu.c32
	APPEND menuconf/diskutil.conf
	
LABEL part
    MENU LABEL > Partition Tools
    KERNEL menu.c32
    APPEND menuconf/part.conf 

LABEL sysUtils
    MENU LABEL > Utilitaires ^systemes
    KERNEL menu.c32
    APPEND menuconf/sysutils.conf 

LABEL LiveCDs
    MENU LABEL > Live CDs
    KERNEL menu.c32
    APPEND menuconf/LiveCDs.conf 
    
LABEL Rescue
    MENU LABEL > Rescue
    KERNEL menu.c32
    APPEND menuconf/rescue.conf   
    
LABEL gPXE
    MENU LABEL > gPXE and iscsi
    KERNEL menu.c32
    APPEND menuconf/gpxe.conf   
   
    
label reboot
MENU LABEL ^Redemarrer
kernel reboot.com

label shutdown
MENU LABEL ^Eteindre
kernel halt.com